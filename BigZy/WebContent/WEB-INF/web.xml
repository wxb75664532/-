<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>JavaWebBigZY</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  
  <filter>
  	<filter-name>LoginFilter</filter-name>
  	<filter-class>servlet.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>LoginFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>

  
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<servlet-class>servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>*.register</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/Login</url-pattern>
  </servlet-mapping>

  <servlet>
  	<servlet-name>PeseicarteServlet</servlet-name>
  	<servlet-class>servlet.PeseicarteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PeseicarteServlet</servlet-name>
  	<url-pattern>*.peseicarte</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>CarteInfoServlet</servlet-name>
  	<servlet-class>servlet.CarteInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CarteInfoServlet</servlet-name>
  	<url-pattern>*.carteInfo</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>TableInfoServlet</servlet-name>
  	<servlet-class>servlet.TableInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>TableInfoServlet</servlet-name>
  	<url-pattern>*.tableInfo</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>RestInfoServlet</servlet-name>
  	<servlet-class>servlet.RestInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RestInfoServlet</servlet-name>
  	<url-pattern>*.restInfo</url-pattern>
  </servlet-mapping>
 
  <servlet>
  	<servlet-name>ReseInfoServlet</servlet-name>
  	<servlet-class>servlet.ReseInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ReseInfoServlet</servlet-name>
  	<url-pattern>*.reseInfo</url-pattern>
  </servlet-mapping>

  
  <servlet>
  	<servlet-name>CommentServlet</servlet-name>
  	<servlet-class>servlet.CommentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CommentServlet</servlet-name>
  	<url-pattern>*.comment</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>